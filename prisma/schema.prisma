  height Int?
  children Int?
  CleaningLevel String? @db.VarChar(255)
  CookingLevel String? @db.VarChar(255)
  WashingLevel String? @db.VarChar(255)
  IroningLevel String? @db.VarChar(255)
  ChildcareLevel String? @db.VarChar(255)
  ElderlycareLevel String? @db.VarChar(255)

}

model Role {
  id          Int       @id @default(autoincrement())
  order         Int?  //ترتيب الصلاحيات  رقم 1 هو اعلى رتبة
  name        String    @unique @db.VarChar(191) // e.g., "مدير عام", "موظف"
  permissions Json?     // Stores permissions as JSON
  users       User[]    // Users assigned to this role
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
}

model User {
  id            Int       @id @default(autoincrement())
  phonenumber   String?   @unique @db.VarChar(15)
  admin         Boolean?  @default(false)
  pictureurl    String?   @default("") @db.LongText
  username      String    @unique @db.VarChar(191)
  password      String    @db.VarChar(191)
  email         String?   @db.VarChar(191)
  roleId        Int?      // Foreign key to Role model
  idnumber      Int?      @unique
  role          Role?     @relation(fields: [roleId], references: [id])
    notifications notifications[]
  tasks         tasks[]
  assignedTasks tasks[] @relation("TaskAssignedBy")
  logs          logs[]
  emaillist    emaillist[]
  readNotifications NotificationRead[]
  cashLogs      cashLogs[]
  SystemLogs    systemUserLogs[]
  DeliveryDetails DeliveryDetails[]
  createdAt     DateTime? @default(now())
  updatedAt     DateTime? @updatedAt

  AccountSystemLogs AccountSystemLogs[]
  createdComplaints Complaint[] @relation("ComplaintCreatedBy")
  assignedComplaints Complaint[] @relation("ComplaintAssignedTo")
}



